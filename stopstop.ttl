<https://songstopoems.github.io/STOP/stopfinal.owl#> rdf:type owl:Ontology ;
                                                      owl:versionIRI <https://songstopoems.github.io/STOP/stopfinal.owl#/1.0> .

#################################################################
#    Object Properties
#################################################################

###  http://schema.org/hasPart
schema:hasPart rdf:type owl:ObjectProperty ;
               owl:inverseOf schema:isPartOf ;
               rdfs:domain stop:Work ;
               rdfs:range stop:Unit .


###  http://schema.org/isPartOf
schema:isPartOf rdf:type owl:ObjectProperty ;
                rdfs:domain stop:Unit ;
                rdfs:range stop:Work .


###  https://songstopoems.github.io/STOP/stopfinal.owl#appears_in
stop:appears_in rdf:type owl:ObjectProperty ;
                owl:inverseOf stop:is_about ;
                rdfs:domain stop:Topic ;
                rdfs:range stop:Unit .


###  https://songstopoems.github.io/STOP/stopfinal.owl#has_character
stop:has_character rdf:type owl:ObjectProperty ;
                   owl:inverseOf stop:is_in ;
                   rdfs:domain stop:Unit ;
                   rdfs:range stop:Character .


###  https://songstopoems.github.io/STOP/stopfinal.owl#inspires
stop:inspires rdf:type owl:ObjectProperty ;
              owl:inverseOf stop:is_inspired_by ;
              rdfs:domain stop:Work ;
              rdfs:range stop:Work .


###  https://songstopoems.github.io/STOP/stopfinal.owl#is_about
stop:is_about rdf:type owl:ObjectProperty ;
              rdfs:domain stop:Unit ;
              rdfs:range stop:Topic .


###  https://songstopoems.github.io/STOP/stopfinal.owl#is_in
stop:is_in rdf:type owl:ObjectProperty ;
           rdfs:domain stop:Character ;
           rdfs:range stop:Unit .


###  https://songstopoems.github.io/STOP/stopfinal.owl#is_inspired_by
stop:is_inspired_by rdf:type owl:ObjectProperty ;
                    rdfs:domain stop:Work ;
                    rdfs:range stop:Work .


###  https://songstopoems.github.io/STOP/stopfinal.owl#is_written_by
stop:is_written_by rdf:type owl:ObjectProperty ;
                   owl:inverseOf stop:writes ;
                   rdfs:domain stop:Work ;
                   rdfs:range stop:Author .


###  https://songstopoems.github.io/STOP/stopfinal.owl#related_to
stop:related_to rdf:type owl:ObjectProperty ;
                rdfs:domain stop:Song ;
                rdfs:range stop:Poem .


###  https://songstopoems.github.io/STOP/stopfinal.owl#writes
stop:writes rdf:type owl:ObjectProperty ;
            rdfs:domain stop:Author ;
            rdfs:range stop:Work .


#################################################################
#    Data properties
#################################################################

###  https://songstopoems.github.io/STOP/stopfinal.owl#have_name
stop:have_name rdf:type owl:DatatypeProperty ;
               rdfs:domain owl:Thing ;
               rdfs:range rdfs:Literal .


#################################################################
#    Classes
#################################################################

